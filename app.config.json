{
  "environmentUrl": "https://lvz97093.sprint.apps.dynatracelabs.com/",
  "app": {
    "name": "Bulk Settings Change",
    "version": "1.1.0",
    "description": "Apply settings 2.0 changes in bulk across multiple entities.",
    "id": "my.bulk.settings.change",
    "scopes": [
      { "name": "storage:logs:read", "comment": "default template" },
      { "name": "storage:buckets:read", "comment": "default template" },
      { "name": "environment-api:entities:read", "comment": "read entities" },
      { "name": "settings:schemas:read", "comment": "read schemas" },
      { "name": "settings:objects:write", "comment": "write settings" },
      { "name": "storage:events:write", "comment": "write audit events" }
    ],
    "csp": {
      "img-src":[
        {
          "value": "https://raw.githubusercontent.com",
          "comment": "allows images from github"
        }
      ]
    }
  },
  "icon": "./src/assets/app-icon.png"
}